@article{Hessler2005,
	Abstract = {Apoptosis is defined by a distinct set of morphological changes observed during cell death including loss of focal adhesions, the formation of cell membrane buds or blebs, and a decrease in total cell volume. Recent studies suggest that these dramatic morphological changes, particularly apoptotic volume decrease (AVD), are an early prerequisite to apoptosis and precede key biochemical time-points. Here we use atomic force microscopy to observe early stage AVD of KB cells undergoing staurosporine-induced apoptosis. After a 3-h exposure to 1 microM staurosporine, a 32\% decrease in total cell height and a 50\% loss of total cell volume is observed accompanied by only a 15\% change in cell diameter. The observed AVD precedes key biochemical hallmarks of apoptosis such as loss of mitochondrial membrane potential, phosphatidyl serine translocation, nuclear fragmentation, and measurable caspase-3 activity. This suggests that morphological volume changes occur very early in the induction of apoptosis.},
	Author = {Hessler, Jessica A. and Budor, Andrew and Putchakayala, Krishna and Mecke, Almut and Rieger, Daniel and {Banaszak Holl}, Mark M. and Orr, Bradford G. and Bielinska, Anna and Beals, James and Baker, Jr, James},
	Doi = {10.1021/la051837g},
	Institution = {Department of Chemistry, and the Michigan Nanotechnology Institute for Medicine and Biological Sciences, University of Michigan, Ann Arbor, MI 48109-1055, USA.},
	Journal = {Langmuir},
	Keywords = {Apoptosis, physiology; Cell Membrane, metabolism/ultrastructure; Cell Nucleus, metabolism/ultrastructure; Cell Size; Humans; KB Cells, metabolism/ultrastructure; Microscopy, Atomic Force, methods; Mitochondria, metabolism/ultrastructure; Phosphatidylserines, metabolism; Time Factors},
	Language = {eng},
