	Keywords = {Amino Acid Chloromethyl Ketones, pharmacology; Animals; Apoptosis, physiology; Caspase Inhibitors; Caspases, physiology; Cell Membrane, drug effects/physiology; Cell Size; Cells, Cultured; Cysteine Proteinase Inhibitors, pharmacology; Electrophysiology; Enzyme Inhibitors, pharmacology; Glucocorticoids, pharmacology; Male; Ouabain, pharmacology; Rats; Rats, Sprague-Dawley; Sodium-Potassium-Exchanging ATPase, antagonists /&/ inhibitors/metabolism; Thymus Gland, cytology/drug effects/physiology},
	Language = {eng},
