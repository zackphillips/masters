	Journal = {Journal of the Optical Society of America A: Optics, Image Science, and Vision},
	Keywords = {Mathematical methods in physics; Radiometry; ABCD transforms ; Transforms; Waves},
	Month = oct,
	Number = {10},
	Owner = {Lei},
