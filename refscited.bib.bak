	Publisher = {The Royal Society of Chemistry},
	Timestamp = {2014.02.12},
	Title = {Lensfree holographic imaging for on-chip cytometry and diagnostics},
	Volume = {9},
	Year = {2009},
	Bdsk-Url-1 = {http://dx.doi.org/10.1039/B813943A}}

