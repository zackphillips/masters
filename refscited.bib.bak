	Volume = {78},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0143816615002122},
	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.optlaseng.2015.09.009}}

