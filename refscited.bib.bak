	Keywords = {Amino Acid Chloromethyl Ketones, pharmacology; Antigens, CD95, pharmacology; Apoptosis, physiology/radiation effects; Biological Transport, drug effects; Caspase 8; Caspase 9; Caspase Inhibitors; Caspases, physiology; Cell Size, drug effects/physiology; Cysteine Proteinase Inhibitors, pharmacology; Enzyme Activation; Humans; Jurkat Cells; Oligopeptides, pharmacology; Potassium, metabolism; Ultraviolet Rays},
	Language = {eng},
	Medline-Pst = {ppublish},
	Month = oct,
	Number = {40},
	Owner = {Laura},
	Pages = {37602--37611},
	Pii = {M104810200},
	Pmid = {11431480},
	Timestamp = {2013.11.06},
	Title = {Differential involvement of initiator caspases in apoptotic volume decrease and potassium efflux during Fas- and {UV}-induced cell death.},
