	Keywords = {Amiloride, analogs /&/ derivatives/diagnostic use/pharmacology; Animals; Antiporters; Biological Transport; Calcium Channels, physiology; Calcium, pharmacokinetics; Carrier Proteins, analysis/antagonists /&/ inhibitors/physiology; Cell Membrane, physiology; Epithelium, physiology; Ion Exchange; Potassium, pharmacokinetics; Sodium Channels, physiology; Sodium, pharmacokinetics; Sodium-Potassium-Exchanging ATPase, physiology; Structure-Activity Relationship},
	Language = {eng},
