	Number = {7329},
	Owner = {Laura},
	Pages = {226--230},
	Pii = {nature09642},
	Pmid = {21196934},
	Timestamp = {2013.11.06},
	Title = {Hydrostatic pressure and the actomyosin cortex drive mitotic cell rounding.},
	Volume = {469},
