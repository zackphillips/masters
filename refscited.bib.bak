@article{Pan:2003zk,
	Abstract = {Digital holography appears to be a strong contender as the next-generation technology for holographic diagnostics of particle fields and holographic particle image velocimetry for flow field measurement. With the digital holographic approach, holograms are directly recorded by a digital camera and reconstructed numerically. This not only eliminates wet chemical processing and mechanical scanning, but also enables the use of complex amplitude information inaccessible by optical reconstruction, thereby allowing flexible reconstruction algorithms to achieve optimization of specific information. However, owing to the inherently low pixel resolution of solid-state imaging sensors, digital holography gives poor depth resolution for images, a problem that severely impairs the usefulness of digital holography especially in densely populated particle fields. This paper describes a technique that significantly improves particle axial-location accuracy by exploring the reconstructed complex amplitude information, compared with other numerical reconstruction schemes that merely mimic traditional optical reconstruction. This novel method allows accurate extraction of particle locations from forward-scattering particle holograms even at high particle loadings.},
	Author = {Gang Pan and Hui Meng},
