	Number = {15},
	Owner = {Laura},
	Pages = {2423--2430},
	Pmid = {10229193},
	Timestamp = {2013.11.06},
	Title = {Caspase-mediated cleavage of cytoskeletal actin plays a positive role in the process of morphological apoptosis.},
	Volume = {18},
	Year = {1999},
	Bdsk-Url-1 = {http://dx.doi.org/10.1038/sj.onc.1202558}}

