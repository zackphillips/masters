@article{10.1371/journal.pone.0098781,
	Abstract = {<p>Here we describe an ultra-low-cost origami-based approach for large-scale manufacturing of microscopes, specifically demonstrating brightfield, darkfield, and fluorescence microscopes. Merging principles of optical design with origami enables high-volume fabrication of microscopes from 2D media. Flexure mechanisms created via folding enable a flat compact design. Structural loops in folded paper provide kinematic constraints as a means for passive self-alignment. This light, rugged instrument can survive harsh field conditions while providing a diversity of imaging capabilities, thus serving wide-ranging applications for cost-effective, portable microscopes in science and education.</p>},
	Author = {Cybulski, James S. AND Clements, James AND Prakash, Manu},
	Doi = {10.1371/journal.pone.0098781},
	Journal = {PLOS ONE},
	Month = {06},
